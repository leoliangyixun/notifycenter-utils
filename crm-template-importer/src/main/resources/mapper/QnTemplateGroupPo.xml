<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hujiang.notifycenter.utils.template.dao.QnTemplateGroupDao">
	<resultMap id="BaseResultMap" type="com.hujiang.notifycenter.utils.template.model.po.QnTemplateGroupPo">
		<result column="app_key" property="appKey" />
		<result column="strategy_id" property="strategyId" />
		<result column="title" property="title" />
		<result column="template_info" property="templateInfo" />
		<result column="status" property="status" />
	</resultMap>

	<insert id="insert" parameterType="com.hujiang.notifycenter.utils.template.model.po.QnTemplateGroupPo" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO template_group (app_key, strategy_id, title, template_info, status)
		VALUES (#{appKey}, #{strategyId}, #{title}, #{templateInfo}, #{status})
	</insert>

	<insert id="insertBatch" parameterType="java.util.List"  useGeneratedKeys="true" keyProperty="id" >
	
		INSERT INTO template_group (app_key, strategy_id, title, template_info, status) VALUES
			<foreach collection="list"  item="item" separator="," >
				 (#{item.id},#{item.appKey},#{item.strategyId},#{item.title},#{item.templateInfo},#{item.status})
			</foreach>
	</insert>
	
</mapper>